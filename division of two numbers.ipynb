    "x = int(input('Enter the numerator '))\n",
    "y = int(input('Enter the denominator '))\n",
    "a = 1\n",
    "b = x\n",
    "while a < x:\n",
    "    z = b - y\n",
    "    b = z\n",
    "    if b < y:\n",
    "        break\n",
    "    a += 1\n",
    "print(f'quotient is {a}')\n",
    "print(f'reminder is {b}')"
